{
    "name": "Employee timekeeping report",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1.3",
        "phpoffice/phpspreadsheet": "^1.0",
        "sensio/framework-extra-bundle": "^5.0",
        "symfony/console": "^4.0",
        "symfony/debug-bundle": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/maker-bundle": "^1.0",
        "symfony/process": "^4.0",
        "symfony/translation": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/yaml": "^4.0",
        "twbs/bootstrap": "^4.0@beta"
    },
    "require-dev": {
        "symfony/dotenv": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "@add-bootstrap"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "@add-bootstrap"
        ],
        "add-bootstrap": "mkdir -p public/bootstrap; cp -r $(pwd)/vendor/twbs/bootstrap/dist/ public/bootstrap/dist; cp -r $(pwd)/vendor/twbs/bootstrap/assets/ public/bootstrap/assets;"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01BZ7EM6P23T009QK50YFK3AGB",
            "allow-contrib": false
        }
    }
}
